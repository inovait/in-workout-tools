{"kind":"symbol","schemaVersion":{"minor":3,"major":0,"patch":0},"hierarchy":{"paths":[[]]},"metadata":{"externalID":"FeatureMedia","roleHeading":"Framework","modules":[{"name":"FeatureMedia"}],"title":"FeatureMedia","role":"collection","symbolKind":"module"},"primaryContentSections":[{"kind":"content","content":[{"text":"Key Concepts","level":2,"type":"heading","anchor":"Key-Concepts"},{"type":"orderedList","items":[{"content":[{"inlineContent":[{"code":"Media","type":"codeVoice"},{"text":" Represents a single media object with its values.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Player: If media type is video, player is set.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Player status: Status of player, tells if video is prepared for playing or not.","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Video manager: Dependency wrapper of video manager, caches videos, prepares it and gets it from cache."}],"type":"paragraph"}]}]},{"anchor":"Functionality","type":"heading","text":"Functionality:","level":2},{"type":"orderedList","items":[{"content":[{"inlineContent":[{"type":"text","text":"Prepares media:"}],"type":"paragraph"}]}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"Prepares media, extracts videos from media array and triggers video manager to cache it.","type":"text"}]}]}]},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"Sets next, previous or specific media:","type":"text"}]}]}],"type":"orderedList","start":2},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Gets next, previous or specific media from array, retreives it from cache."}]}]}],"type":"unorderedList"},{"type":"orderedList","items":[{"content":[{"inlineContent":[{"text":"Playback functionality:","type":"text"}],"type":"paragraph"}]}],"start":3},{"type":"unorderedList","items":[{"content":[{"t